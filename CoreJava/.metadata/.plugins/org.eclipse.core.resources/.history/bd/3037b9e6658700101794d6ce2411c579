
package core.main;
/*

public static returnType funcName(list of params)
{
		block of code
	return data/var;
}

Q1. Factors?
Q2. Factorial?
Q3. maximum b/w 4 var?
Q4. reverse a num?
Q5. power?
Q6. palindrome?




*/




public class Func1 {

	public static int maximum(int a, int b) {
		return (a > b) ? a : b;
	}

	public static void isPrime(int n) {
		boolean b = true;
		for (int i = 2; i < n; i++) {
			if (n % i == 0) {
				b = false;
				break;
			}
		}
		if (b) {
			System.out.println("Prime");
		} else {
			System.out.println("Not Prime");
		}
		return;
	}
	
	public static void Facts(int n ) {
		
		
		for (int i = 1; i <= n ; i++) {
			if(n%i==0) {
				System.out.println(i + " ,");
			}
		}
		return;
	}

	public static void main(String[] args) {

		int n = 13, n2 = 333, n3 = 77;
		isPrime(n);
		isPrime(n2);
		isPrime(n3);
		isPrime(43);
		isPrime(433);
		isPrime(56);
		isPrime(221);
		isPrime(91);
		Facts(n);
		Facts(n);
		Facts(n2);
		Facts(n3);
		Facts(43);
		Facts(433);
		Facts(56);
		Facts(221);
		Facts(91);

		int m = maximum(n2, n3);
		
		System.out.println(m);
		System.out.println(maximum(3333, m));
	}
}
