package core.main;



// https://leetcode.com/problems/find-smallest-letter-greater-than-target/
public class SmallestLetterGreaterThanTar {
	
	public char nextGreatestLetter(char[] letters, char target) {
		
		int s = 0;
		int N = letters.length-1;
		int e = N;
		while(s <= e)
		{
			int m = (s + e)/2;
			
			if  (target < letters[m]) {
				e = m - 1;
				
			}else if(target > letters[m]) {
				s = m + 1;
			}else if(target == letters[m]) {
				char c = letters[m+1];
				return c;
				
			}else if(s == N)
		}
		
		return letters[s];
	}

	public static void main(String[] args) {
		

	}

}
