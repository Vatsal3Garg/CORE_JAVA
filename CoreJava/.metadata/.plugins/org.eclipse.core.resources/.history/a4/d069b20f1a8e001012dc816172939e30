package core.main;

	
public class Student {

	 private String name;
	 private int age;
	 private String add;
	 private int roll;
	
	
	public void print() {
		System.out.println("Name is : "+ name);
		System.out.println("Age is : "+ age);
		System.out.println("Address is : "+ add);
		System.out.println("Roll Number is : "+ roll);
	
		
		
	}
	
	
	
	
	
	public static void main(String[] args) {
		

		
		Student s1 = new Student();
		s1.name = "Vatsal Garg";
		s1.age = 33;
		s1.add = "Greater Noida";
		s1.roll = 170;
		
		s1.print();
		
		
		
		Student s2 = new Student();
		s2.name = "Vikas Kumar";
		s2.age = 23;
		s2.add = "Ghaziabad";
		s2.roll = 180;
		s2.print();
		
		
	
	}

}
