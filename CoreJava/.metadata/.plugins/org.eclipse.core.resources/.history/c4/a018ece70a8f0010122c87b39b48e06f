package core.main;

public class BinarySearch {
	
	public void binarysearch(int[] arr , int target ) {
		
		int start = 0 ;
		int end = arr.length-1;
		int m = (start + end)/2;
		if (target > m ) {
			start = m+1;
			end = arr.length-1;
			
		}else if(target < m ){
			end = m-1;
			start = 0;
		}else if(target == m) {
			return 
			
		}
		
		
		
		
	}

	public static void main(String[] args) {
		
	}

}
